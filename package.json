{
  "name": "auto-mart",
  "version": "1.0.0",
  "description": "Auto Mart is an online marketplace for automobiles of diverse makes, model or body type. With Auto Mart, users can sell their cars or buy from trusted dealerships or private sellers",
  "main": "index.js",
  "scripts": {
    "build": "babel ./server --out-dir build",
    "lint": "npx eslint ./ --fix",
    "start": "npm run build && node ./build/app.js",
    "dev": "nodemon --exec babel-node ./server/app",
    "test": "nyc mocha --require @babel/register ./server/tests --exit",
    "coverage": "nyc npm test && nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hustlaviola/Auto-Mart.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/hustlaviola/Auto-Mart/issues"
  },
  "homepage": "https://github.com/hustlaviola/Auto-Mart#readme",
  "devDependencies": {
    "@babel/node": "^7.4.5",
    "@babel/register": "^7.4.4",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "coveralls": "^3.0.4",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.17.3",
    "mocha": "^6.1.4",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.19.1",
    "nyc": "^14.1.1"
  },
  "dependencies": {
    "bcrypt": "^3.0.6",
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1"
  }
}
